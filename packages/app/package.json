{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "run-p build:*",
    "build:app": "./scripts/build.sh --app=vite",
    "build:crx": "./scripts/build.sh --app=crx",
    "build:storybook": "./scripts/build.sh --app=storybook",
    "dev": "vite",
    "dev:crx": "vite --config vite.crx.config.ts",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "test": "jest --verbose",
    "test:ci": "pnpm ts:check && pnpm test",
    "test:watch": "jest --watch",
    "ts:check": "pnpm xstate:typegen && tsc --noEmit",
    "xstate:typegen": "xstate typegen 'src/**/*.ts?(x)'",
    "graphql:codegen": "graphql-codegen && prettier --write src/api/__generated__/types.ts",
    "graphql:schema": "get-graphql-schema http://localhost:4000/graphql > fuel-core-schema.graphql && prettier --write fuel-core-schema.graphql",
    "postinstall": "pnpm graphql:codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@fontsource/source-code-pro": "^4.5.12",
    "@fuel-ts/mnemonic": "0.28.0",
