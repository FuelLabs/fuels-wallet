{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "tsc && vite build && pnpm create404",
    "build:preview": "run-s build build-storybook",
    "create404": "cp ./dist/index.html ./dist/404.html",
    "dev": "vite",
    "preview": "vite preview",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch --runInBand --detectOpenHandles",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "pnpm exec build-storybook -o dist/storybook"
  },
  "dependencies": {
    "@fontsource/source-code-pro": "^4.5.11",
    "@fuel-ui/css": "^0.5.0",
    "@fuel-ui/react": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-jest": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-storysource": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/api": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/components": "^6.5.10",
    "@storybook/core-events": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.10",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-loader": "^8.2.5",
    "jest-fail-on-console": "^2.5.0",
    "storybook-dark-mode": "^1.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-tsconfig-paths": "^3.5.0",
    "webpack": "5"
  },
  "pnpm": {
    "overrides": {
