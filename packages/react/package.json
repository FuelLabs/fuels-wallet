{
  "name": "@fuel-wallet/react",
  "description": "Interact with the Fuel Wallet Extension in React.",
  "version": "0.13.4",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "require": "dist/index.js",
        "default": "dist/index.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm ts:check && tsup",
    "ts:check": "pnpm tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "@fuel-ui/css": "0.21.1",
    "@fuel-ui/react": "0.21.1",
    "fuels": ">=0.63.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.36.1",
    "@fuel-wallet/sdk": "workspace:*",
    "@fuel-wallet/types": "workspace:*",
    "compare-versions": "^6.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "tsup": "^7.2.0",
    "fuels": "0.63.0"
  }
}
